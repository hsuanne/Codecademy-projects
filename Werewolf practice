//In this program, I practiced coding with if/else statements, logical operators, and switch statements.
let moonPhase = 'full';
let isFoggyNight = false;

if (moonPhase === 'full' || isFoggyNight === true) {
  console.log('How!');
} else if (moonPhase === 'mostly full') {
  console.log('Arms and legs are getting hairier.');
} else if (moonPhase === 'mostly new') {
  console.log('Back on two feet.')
} else {
  console.log('Invalid moon phase')
}

//switch statements
let moonPhase = 'full';

switch (moonPhase) {
  case 'mostly full':
    console.log('Arms and legs are getting hairier.');
    break;
  case 'mostly new':
    console.log('Back on two feet.');
    break;
  case 'full':
    console.log('How!');
    break;
  default:
    consolt.log('Invalid moon phase');
    break;
}
           
